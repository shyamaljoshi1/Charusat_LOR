generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model tblPersonalInfo {
  studentId        String        @unique
  studentName      String
  emailId          String
  studentMobile    String
  parentMobile     String
  dateOfGraduation String
  studentPlace     Boolean
  companyName      String        @default("NA")
  bond             Boolean
  noOfLetterHead   Int           @default(0)
  aid              Int
  rid              Int
  sid              Int           @id @default(autoincrement())
  attendances      tblAttendance @relation(fields: [aid], references: [id])
  results          tblResult     @relation(fields: [rid], references: [id])
}

model tblResult {
  id          Int               @id @default(autoincrement())
  studentId   String
  rsem1       String
  rsem2       String
  studentInfo tblPersonalInfo[]
}

model tblAttendance {
  id          Int               @id @default(autoincrement())
  studentId   String
  asem1       String
  asem2       String
  studentInfo tblPersonalInfo[]
}
