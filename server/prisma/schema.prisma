generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model tblPersonalInfo {
  studentId        String   @unique
  studentName      String
  emailId          String
  studentMobile    String
  parentMobile     String
  dateOfGraduation DateTime @default(now())
}
